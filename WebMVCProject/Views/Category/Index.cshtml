@using ModelLayer.Entities
@using ModelLayer.CategoriesEntities
@model List<Category>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@foreach (var item in Model)
{
    <p>Id : @item.Id</p>
    <p>Description : @item.Description</p>
    <p>Name : @item.Name</p>
    <p>----------------------------------------</p>
}

